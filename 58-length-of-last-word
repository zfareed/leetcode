/**
 * @param {string} s
 * @return {number}
 */

var lengthOfLastWord = function(s) {
    let list = s.split(" ")
    list.reverse()
    for (let i = 0; i < list.length; i++) {
      if (list[i] != 0) {
        return list[i].length
      }
    }
    
};


let res = lengthOfLastWord("   fly me   to   the moon  ")
console.log(res);